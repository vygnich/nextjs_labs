{
  "name": "textile-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prod": "env-cmd -f .env.prod next dev",
    "dev": "env-cmd -f .env.dev next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "db:generate": "env-cmd -f .env.dev prisma generate",
    "db:migrate": "env-cmd -f .env.dev prisma migrate dev",
    "db:migrate:prod": "env-cmd -f .env.prod prisma migrate dev",
    "db:studio": "env-cmd -f .env.dev prisma studio",
    "db:studio:prod": "env-cmd -f .env.prod prisma studio",
    "db:seed": "env-cmd -f .env.dev ts-node prisma/seed.ts",
    "db:seed:prod": "env-cmd -f .env.prod ts-node prisma/seed.ts",
    "db:seed:hard": "pnpm run db:reset && pnpm run db:seed",
    "db:reset": "env-cmd -f .env.dev ts-node prisma/db-reset.ts",
    "db:reset:prod": "env-cmd -f .env.prod ts-node prisma/db-reset.ts",
    "db:push": "env-cmd -f .env.dev prisma db push",
    "db:push:prod": "env-cmd -f .env.prod prisma db push",
    "db:sync": "pnpm run db:reset && pnpm run db:generate && pnpm run db:migrate && pnpm run db:push",
    "prepare": "echo 'husky prepare'",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage --silent",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@auth/prisma-adapter": "^2.0.0",
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@sentry/nextjs": "^7.112.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.32.0",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "^15.0.5",
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.1.4",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-mock-extended": "^3.0.6",
    "jsdom": "^24.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.376.0",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "postcss": "^8",
    "prisma": "^5.13.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.1.0",
    "resend": "^3.2.0",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.3",
    "zod": "^3.23.5",
    "zod-prisma": "^0.5.4"
  }
}
